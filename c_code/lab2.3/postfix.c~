#include <stdio.h>
#include "stack.c"
#define STACK_SIZE 100

int main(){
  int ch;
  int sp = 0;
  int input = 0;
  int num = -1;
  int p1;
  int p2;
  
  while (input != EOF){
    if (isdigit(ch)){
      num = 0;
      num = num * 10 + (ch - '0');
    }

    else if (input == ' '){
      if (num == 0){
	push(num);
	num = -1;
      }
    }
    
    else if (operator == '+' || operator == '-' ||
	     operator == '*' || operator == '/') {
      if (stack < 2) {
	printf("There are too few operators in the expression.\n");
	return -1;
	p1 = pop();
	p2 = pop();
	
	if (operator == '+') {
	  push(p1 + p2);
	  printf("Result: ");
	  return 0;
      }
      
	

    } 
 


  }
